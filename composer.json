{
  "name": "AndriusKuc/laravel42-encrypter",
  "type": "library",
  "config": {
    "preferred-install": "dist",
    "bin-dir": "bin",
    "process-timeout": 10000,
    "github-protocols": ["https"]
  },
  "require": {
    "php": ">=5.6.4",
    "ext-openssl": "*",
    "paragonie/random_compat": "~1.4|~2.0"
  },
  "require-dev": {
    "laravel/laravel": "~4.2.0",
    "phpunit/phpunit": "~4.0|~5.7"
  },
  "autoload": {
    "psr-4": {
      "AndriusKuc\\Encryption\\": "src/AndriusKuc/Encryption/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}